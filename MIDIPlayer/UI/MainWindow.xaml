        <StackPanel Background="#FF151F27" x:Name="infoPanel" Grid.Row="1" Grid.Column="{Binding InfoControlColumn}" Grid.ColumnSpan="3" Visibility="{Binding ShowInfo, Converter={StaticResource BoolToVisibilityConverter}}" >
            <ui:InfoControl x:Name="infoControl" />
        </StackPanel>

        <StackPanel x:Name="logPanel" Grid.Row="1" Margin="5,0,0,0" Grid.Column="{Binding LogControlColumn}" Grid.ColumnSpan="3" Visibility="{Binding ShowLog, Converter={StaticResource BoolToVisibilityConverter}}" >
            <ui:LogControl x:Name="logControl" />
        </StackPanel>





        <!--<GridSplitter ResizeDirection="Rows" Width="1000" Height="10" Grid.Row="3" HorizontalAlignment="Center" VerticalAlignment="Stretch" Grid.ColumnSpan="3" Visibility="{Binding ShowLog, Converter={StaticResource BoolToVisibilityConverter}}" Margin="-178,0.333,-178.667,80.333" Grid.RowSpan="2"></GridSplitter>-->

        <StackPanel Grid.Row="2" Grid.ColumnSpan="3"   Visibility="{Binding PlayerToolbar.IsVisible, Converter={StaticResource BoolToVisibilityConverter}}">


            <Grid Margin="0,1,0,-0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>

                <Label

                        Grid.Column="0"
                    Margin="0,-6,0,0"
                Content="{Binding TimeElapsed}"
                FontSize="12"
                Foreground="#FF1D2F3D"
                Visibility="{Binding ShowTimer, Converter={StaticResource BoolToVisibilityConverter}}" HorizontalAlignment="Center" />

                <Slider
                    IsMoveToPointEnabled="True"
                    Grid.Column="1" 
                    HorizontalAlignment="Stretch"
                    Margin="2,-6,0,0"
                IsSelectionRangeEnabled="False"
                x:Name="SeekSlider"
                Foreground="#FF727683"
                Height="17" 
                Minimum="0"
                Maximum="{Binding SequenceLength}"
                Value="{Binding SeekValue, Mode=TwoWay}"
                IsEnabled="{Binding IsSeekSliderEnabled}"
                Visibility="{Binding ShowSeekSlider, Converter={StaticResource BoolToVisibilityConverter}}" 
                TickPlacement="None" 
     
                GotMouseCapture="SeekSlider_GotMouseCapture" 
                LostMouseCapture="SeekSlider_LostMouseCapture" Style="{DynamicResource SliderStyle}" 
                >
                    <i:Interaction.Triggers>
                        <i:EventTrigger EventName="ValueChanged">
                            <cmd:EventToCommand PassEventArgsToCommand="True" Command="{Binding SeekChangedCommand}"/>
                        </i:EventTrigger>
                        <i:EventTrigger EventName="MouseWheel">
                            <cmd:EventToCommand PassEventArgsToCommand="True" Command="{Binding SeekMouseWheelCommand}"/>
                        </i:EventTrigger>
                        <i:EventTrigger EventName="MouseEnter">
                            <cmd:EventToCommand PassEventArgsToCommand="True" Command="{Binding SeekMouseEnterCommand}"/>
                        </i:EventTrigger>
                        <i:EventTrigger EventName="PreviewMouseLeftButtonDown">
                            <cmd:EventToCommand PassEventArgsToCommand="True" Command="{Binding SeekMouseLeftButtonDownCommand}"/>
                        </i:EventTrigger>
                        <i:EventTrigger EventName="MouseLeave">
                            <cmd:EventToCommand PassEventArgsToCommand="True" Command="{Binding SeekMouseLeaveCommand}"/>
                        </i:EventTrigger>

                    </i:Interaction.Triggers>
                </Slider>


                <Label
                        DockPanel.Dock="Right"
          
                        Grid.Column="2"
                    Margin="0,-6,0,0"
                Height="29"
                Content="{Binding TimeLeft}"
                FontSize="12"
                Foreground="#FF1D2F3D"
                Visibility="{Binding ShowTimer, Converter={StaticResource BoolToVisibilityConverter}}" HorizontalAlignment="Center"/>

            </Grid>



            <Border
                            Margin="0,0,0,0"
                BorderBrush="{DynamicResource {x:Static SystemColors.ControlDarkBrushKey}}" 
                BorderThickness="1"
                HorizontalAlignment="Stretch"
                Height="1" 
                VerticalAlignment="Top"/>

            <StackPanel Orientation="Horizontal" Margin="5,-4,0,0">
                <ToolBar
                    Padding="0,0,0,0"
                    Panel.ZIndex="-9999"
                ToolBar.OverflowMode="Never"
               ToolBarTray.IsLocked="True"
                HorizontalAlignment="Left"
                    Margin="-8,-5,0,0"
                Background="#FFF0F6FF"
                Visibility="{Binding PlayerToolbar.IsVisible, Converter={StaticResource BoolToVisibilityConverter}}">

                    <Button
                    x:Name="btnPlay"
                    Height="50"
                    Click="btnPlay_Click"
                    ToolTip="Play"
                    Visibility="{Binding PlayerToolbar.ShowPlayButton, Converter={StaticResource BoolToVisibilityConverter}}">
                        <Image Source="../Images/icons8-play-50.png" />
                    </Button>
                    <Button
                    x:Name="btnPause"
                    Height="50"
                    Click="btnPause_Click"
                    ToolTip="Pause"
                    Visibility="{Binding PlayerToolbar.ShowPauseButton, Converter={StaticResource BoolToVisibilityConverter}}">
                        <Image Source="../Images/icons8-pause-50.png" />
                    </Button>

                    <Button
                     
                        IsEnabled="{Binding PlayerToolbar.StopButtonEnabled}"
                    x:Name="btnStop"
                    Height="50"
                    Click="btnStop_Click"
                    ToolTip="Stop">
                        <Image Source="../Images/icons8-stop-50.png" />
                    </Button>

                    <Button
                    x:Name="btnPrevious"
                    Height="50"
                    Click="btnPrevious_Click"
                    ToolTip="Previous">
                        <Image Source="../Images/icons8-skip-to-start-50.png" />
                    </Button>
                    <Button
                    x:Name="btnNext"
                    Height="50"
                    Click="btnNext_Click"
                    ToolTip="Next">
                        <Image Source="../Images/icons8-end-50.png" />
                    </Button>


                    <ToggleButton
                    Command="{Binding Playlist.ToggleRepeatModeCommand}"
                    IsChecked="true"
                    Height="30"
                    ToolTip="{Binding Playlist.RepeatModeToolTip}">
                        <Image Source="{Binding Playlist.RepeatModeImage}" />
                    </ToggleButton>

                    <!--<ToggleButton
                    Command="{Binding ToggleLoopAllCommand}"
                    IsChecked="{Binding Playlist.LoopAllIsEnabled}"
                    Height="30"
                    ToolTip="Plays all MIDI's in sequence and restarts when finished">
                        <Image Source="../Images/icons8-repeat-50.png" />
                    </ToggleButton>

                    <ToggleButton
                    Command="{Binding ToggleShuffleCommand}"
                    IsChecked="{Binding Playlist.ShuffleIsEnabled}"
                    x:Name="btnShuffle"
                    Height="30"
                    ToolTip="Shuffles the MIDI's">
                        <Image Source="../Images/icons8-shuffle-50.png" />
                    </ToggleButton>

                    <Separator/>
                    <Button
                    Command="{Binding ReloadSequenceCommand}"
                    Height="30"
                    ToolTip="Reverts all setting changes of the selected MIDI">
                        <Image Source="../Images/icons8-refresh-48.png" />
                    </Button>-->



                    <!--<Separator/>-->

                    <!--<ToggleButton
                    IsChecked="{Binding ShowSongSettings}"
                    Height="30"
                    Command="{Binding ToggleSongSettingsCommand}"
                    ToolTip="Show/hide MIDI settings">
                        <Image Source="../Images/icons8-tune-48.png" />
                    </ToggleButton>-->

                    <Separator/>

                    <StackPanel Orientation="Horizontal" Margin="-5,4,0,0">
                        <Slider Style="{DynamicResource SliderStyle}"
                     ToolTipService.ShowDuration="5000"
                    x:Name="tempoSlider"
                    Margin="10,-4,0,-1.333"
                IsMoveToPointEnabled="True"
                IsSelectionRangeEnabled="False"
                Width="100" Foreground="#FF727683" 
                Minimum="1"
                Maximum="200"
                Value="{Binding Tempo, Mode=OneWay}"
                IsSnapToTickEnabled="True"
                TickFrequency="1"
                                ToolTip="{Binding TempoText}"
                Grid.ColumnSpan="2" HorizontalAlignment="Center"  VerticalAlignment="Center" >
              
                            <i:Interaction.Triggers>
                                <i:EventTrigger EventName="ValueChanged">
                                    <cmd:EventToCommand PassEventArgsToCommand="True" Command="{Binding TempoChangedCommand}"/>
                                </i:EventTrigger>
                                <i:EventTrigger EventName="PreviewMouseLeftButtonUp">
                                    <cmd:EventToCommand PassEventArgsToCommand="True" Command="{Binding TempoUpdatedCommand}"/>
                                </i:EventTrigger>
                                <i:EventTrigger EventName="MouseWheel">
                                    <cmd:EventToCommand PassEventArgsToCommand="True" Command="{Binding TempoMouseWheelCommand}"/>
                                </i:EventTrigger>
                                <i:EventTrigger EventName="MouseEnter">
                                    <cmd:EventToCommand PassEventArgsToCommand="True" Command="{Binding TempoMouseEnterCommand}"/>
                                </i:EventTrigger>
                                <i:EventTrigger EventName="PreviewKeyDown">
                                    <cmd:EventToCommand PassEventArgsToCommand="True" Command="{Binding TempoKeyDownCommand }"/>
                                </i:EventTrigger>
                                <i:EventTrigger EventName="PreviewKeyUp">
                                    <cmd:EventToCommand PassEventArgsToCommand="True" Command="{Binding TempoKeyUpCommand }"/>
                                </i:EventTrigger>
                            </i:Interaction.Triggers>
                        </Slider>

                        <!--<Label 
                            Margin="-2,-3,0,0"
                            Content="{Binding Tempo}"
                            Foreground="#FF1D2F3D"
                                             FontSize="10" VerticalAlignment="Center"></Label>-->

                        <!--<Label 
                            Margin="-10,-2,0,0"
                            Content="%"
                            VerticalAlignment="Center"
                            Foreground="#FF1D2F3D"
                                             FontSize="10"></Label>-->

                    </StackPanel>
                </ToolBar>
            </StackPanel>


            <Border
                            Margin="0,0,0,0"
                BorderBrush="{DynamicResource {x:Static SystemColors.ControlDarkBrushKey}}" 
                BorderThickness="1"
                HorizontalAlignment="Stretch"
                Height="1" 
                VerticalAlignment="Top"/>

   
            <!--<StackPanel Grid.ColumnSpan="2" HorizontalAlignment="Left">
                <ui:PianoControl
                    
                x:Name="pianoControl"
                Visibility="{Binding ShowPiano, Converter={StaticResource BoolToVisibilityConverter}}"
                BlackKeyColor="#070D12"
                Width="600"
                Height="60"
                HighNoteId="109"
                LowNoteId="21"
                NoteOnColor="#93BDDC"
                StrokeColor="#070D12"
                WhiteKeyColor="#FFF0F6FF" 
                Margin="8,10,0,0"  />

            </StackPanel>-->

        </StackPanel>

        <StackPanel Grid.Row="3" Grid.Column="{Binding SettingsColumn}" Visibility="{Binding SettingsVisible, Converter={StaticResource BoolToVisibilityConverter}}" Grid.ColumnSpan="3"  >
            <ui:SettingsControl x:Name="settingsControl" />
            <Border
                            Margin="0,0,0,0"
                BorderBrush="{DynamicResource {x:Static SystemColors.ControlDarkBrushKey}}" 
                BorderThickness="1"
                HorizontalAlignment="Stretch"
                Height="1" 
                VerticalAlignment="Top"/>
        </StackPanel>

        <StackPanel Margin="5,0,0,0"  Grid.Row="3" Grid.Column="{Binding FfxivControlColumn}" Grid.ColumnSpan="3" Visibility="{Binding ShowFfxiv, Converter={StaticResource BoolToVisibilityConverter}}"   >
            <ui:FfxivControl x:Name="FfxivControl" />

            <Border
                            Margin="0,0,0,0"
                BorderBrush="{DynamicResource {x:Static SystemColors.ControlDarkBrushKey}}" 
                BorderThickness="1"
                HorizontalAlignment="Stretch"
                Height="1" 
                VerticalAlignment="Top"/>

        </StackPanel>

        <Border
                            Margin="0,0,0,0"
                BorderBrush="{DynamicResource {x:Static SystemColors.ControlDarkBrushKey}}" 
                BorderThickness="1"
                HorizontalAlignment="Stretch"
                Height="1" 
                VerticalAlignment="Top"/>
        
        <StackPanel Name="playlistColumn" Grid.Row="4"  Grid.Column="0" Grid.ColumnSpan="3" VerticalAlignment="Top" HorizontalAlignment="Stretch"   MinWidth="400">

            <ui:PlaylistControl
                    AllowDrop="True"
                    x:Name="playlistControl" />

        </StackPanel>

        <!--<GridSplitter Grid.Column="1" Width="10" HorizontalAlignment="Stretch" />-->

        <StackPanel Name="tracksColumn" Grid.Row="4" Grid.Column="2"  Visibility="{Binding TracksVisible, UpdateSourceTrigger=PropertyChanged, Converter={StaticResource BoolToVisibilityConverter}}" HorizontalAlignment="Stretch" VerticalAlignment="Top" Grid.ColumnSpan="2" >
            <TextBox
                    IsEnabled="False"
            Visibility="{Binding IsFilterEnabled, Converter={StaticResource BoolToVisibilityConverter}}"/>
            <ui:TracksControl x:Name="tracksControl" MinWidth="400"/>
        </StackPanel>


        <StackPanel  Margin="5,0,0,0" Grid.Row="5" Grid.ColumnSpan="2" Orientation="Horizontal"  Visibility="{Binding ShowSongSettings, Converter={StaticResource BoolToVisibilityConverter}}">

            <Border BorderBrush="{DynamicResource {x:Static SystemColors.ControlDarkBrushKey}}" BorderThickness="1">

                <StackPanel Orientation="Horizontal">

                    <!--<Label
                                        Margin="5,3,0,0"
                                        Content="Min Length"
                                                     FontSize="10"
                                        Foreground="#FF1D2F3D"  />-->

                    <!--<ui:SpinButton 
                            Enabled="{Binding PlayerToolbar.IsDropDownsEnabled}"
                            HorizontalAlignment="Left" 
                            VerticalAlignment="Top"
                            MinValue="0" 
                            MaxValue="1000" 
                            Value="{Binding MinLength, Mode=TwoWay}" 
                            Increment="5" 
                            Height="20" 
                            Width="60" 
                            Margin="0,5,0,-7.333">
                            <i:Interaction.Triggers>
                                <i:EventTrigger EventName="ValueChanged">
                                    <cmd:EventToCommand PassEventArgsToCommand="True" Command="{Binding MinLengthChangedCommand}"/>
                                </i:EventTrigger>
                            </i:Interaction.Triggers>
                        </ui:SpinButton>-->

                    <CheckBox
                    
                                    Command="{Binding SequenceHighestOnlyCommand}"
                                 Margin="0,6,0,0"
                            x:Name="chkSequenceHighestOnly"
 
                            Content="Highest Only"
                            Foreground="#FF1D2F3D"
                            IsChecked="{Binding HighestOnly}" 
                                             FontSize="10" RenderTransformOrigin="0.48,0.125" />

                    <CheckBox
                   
                            Command="{Binding PlayAllCommand}"
                            Margin="5,6,0,0"
 
                            Content="Play All"
                            Foreground="#FF1D2F3D"
                            IsChecked="{Binding PlayAll}" 
                                             FontSize="10" />

                    <Label
                                        Margin="5,3,0,0"
                                        Content="Max Notes"
                                                     FontSize="10"
                                        Foreground="#FF1D2F3D"  />
                    <ui:SpinButton 
                            Margin="0,5,0,0"
                            Enabled="{Binding ReduceMaxNotesEnabled}"
                            MinValue="2" 
                            MaxValue="8" 
                            Value="{Binding ReduceMaxNotes, Mode=TwoWay}" 
                            Increment="1" 
                            Height="20" 
                            Width="60" 
                            >
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="ValueChanged">
                                <cmd:EventToCommand PassEventArgsToCommand="True" Command="{Binding MaxNotesChangedCommand}"/>
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                    </ui:SpinButton>



                    <Label
                                        Margin="5,3,0,0"
                                        Content="Key Offset"
                                                     FontSize="10"
                                        Foreground="#FF1D2F3D"  />

                    <ui:SpinButton 
                                Enabled="True"
                            HorizontalAlignment="Left" 
                            VerticalAlignment="Top"
                            MinValue="-48" 
                            MaxValue="48" 
                            Value="{Binding KeyOffset, Mode=TwoWay}" 
                            Increment="1" 
                            Height="20" 
                            Width="60" 
                            Margin="0,5,0,-7.333">
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="ValueChanged">
                                <cmd:EventToCommand PassEventArgsToCommand="True" Command="{Binding KeyOffsetChangedCommand}"/>
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                    </ui:SpinButton>

                    <Label
                                        Margin="0,3,0,0"
                                        Content="Octave Offset"
                                                     FontSize="10"
                                        Foreground="#FF1D2F3D"  />
                    <ui:SpinButton 
                        Enabled="True"
                            HorizontalAlignment="Left" 
                            VerticalAlignment="Top"
                            MinValue="-4" 
                            MaxValue="4" 
                            Value="{Binding OctaveOffset, Mode=TwoWay}" 
                            Increment="1" 
                            Height="20" 
                            Width="59" 
                            Margin="0,5,0,0">
                        <i:Interaction.Triggers>

                            <i:EventTrigger EventName="ValueChanged">
                                <cmd:EventToCommand PassEventArgsToCommand="True" Command="{Binding OctaveOffsetChangedCommand}"/>
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                    </ui:SpinButton>


                </StackPanel>
            </Border>
            <StackPanel Orientation="Horizontal" Margin="20,10,0,0"/>
        </StackPanel>



    </Grid>


</Window>
